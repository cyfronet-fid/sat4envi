{
  "name": "s4e-angular",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --configuration hmr",
    "start-private": "ng serve --proxy-config proxy.conf.json --configuration hmr.private",
    "start-production": "ng serve --proxy-config proxy.conf.json --configuration production",
    "start-production-private": "ng serve --proxy-config proxy.conf.json --configuration production.private",
    "build": "ng build",
    "build-private": "ng build --configuration production.private",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "cypress run",
    "extract-i18n": "ng xi18n s4e-angular --i18n-format xlf --output-path i18n --i18n-locale pl && ng run s4e-angular:xliffmerge",
    "start-en": "ng serve --proxy-config proxy.conf.json --configuration=en"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!ol|proj4)/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/app/app.constants.spec.ts",
      "<rootDir>/src/app/app.configuration.spec.ts",
      "\\.factory.spec.ts$"
    ],
    "transform": {
      "(?!<rootDir>/node_modules)/.*\\.js$": "babel-jest"
    }
  },
  "babelOptions": {
    "compact": false,
    "exclude": "/node_modules/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.4",
    "@angular/cdk": "^7.2.4",
    "@angular/common": "^7.2.4",
    "@angular/compiler": "^7.2.4",
    "@angular/core": "^7.2.4",
    "@angular/forms": "^7.2.4",
    "@angular/http": "^7.2.4",
    "@angular/platform-browser": "^7.2.4",
    "@angular/platform-browser-dynamic": "^7.2.4",
    "@angular/router": "^7.2.4",
    "@datorama/akita": "^4.17.1",
    "@datorama/akita-ng-forms-manager": "^3.3.0",
    "@datorama/akita-ng-router-store": "^4.0.0",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@ng-stack/forms": "^1.2.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "core-js": "^2.6.4",
    "date-fns": "^2.0.0-alpha.27",
    "jspdf": "^1.5.3",
    "moment": "^2.24.0",
    "ng-pick-datetime": "^7.0.0",
    "ng-recaptcha": "^5.0.0",
    "ngx-take-until-destroy": "^5.4.0",
    "ol": "^5.3.3",
    "proj4": "^2.5.0",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-builders/jest": "^7.3.1",
    "@angular-devkit/build-angular": "^0.8.9",
    "@angular/cli": "^7.0.7",
    "@angular/compiler-cli": "^7.2.4",
    "@angular/language-service": "^7.2.4",
    "@angularclass/hmr": "^2.1.3",
    "@babel/preset-env": "^7.4.2",
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@datorama/akita-ngdevtools": "^3.0.2",
    "@fortawesome/fontawesome-free": "^5.7.1",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.2",
    "@ngx-i18nsupport/tooling": "^1.1.2",
    "@types/node": "~8.9.4",
    "@types/ol": "^5.3.3",
    "@types/openlayers": "^4.6.15",
    "@types/proj4": "^2.5.0",
    "akita-schematics": "^2.0.1",
    "babel-jest": "^24.5.0",
    "bootstrap": ">=4.3.1",
    "codelyzer": "~4.2.1",
    "cypress": "^3.2.0",
    "factory.ts": "^0.4.5",
    "jest": "^24.5.0",
    "jest-canvas-mock": "^2.0.0-beta.1",
    "jest-marbles": "^2.3.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "^3.1.6"
  }
}
