{
  "name": "s4e-angular",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "gen-vars": "node node_modules/ng-node-environment/index.js",
    "make-requirements": "npm run gen-vars && npm run build-libraries",
    "start-notifications": "ng build notifications --watch",
    "build-libraries": "ng build notifications",
    "prestart": "npm run make-requirements",
    "start": "ng serve --proxy-config proxy.conf.json --configuration hmr",
    "prestart-no-persistance": "npm run make-requirements",
    "start-no-persistance": "ng serve --configuration hmr-no-persist --proxy-config proxy.conf.json",
    "prestart-private": "npm run make-requirements",
    "start-private": "ng serve --proxy-config proxy.conf.json --configuration hmr.private",
    "prestart-production": "npm run make-requirements",
    "start-production": "ng serve --proxy-config proxy.conf.json --configuration production",
    "prestart-production-private": "npm run make-requirements",
    "start-production-private": "ng serve --proxy-config proxy.conf.json --configuration production.private",
    "prebuild": "npm run make-requirements",
    "build": "ng build --prod",
    "prebuild-private": "npm run make-requirements",
    "build-private": "ng build --configuration production.private",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "cypress run",
    "e2e-browser": "cypress run --browser chrome",
    "extract-i18n": "ng xi18n s4e-angular --i18n-format xlf --output-path i18n --i18n-locale pl && ng run s4e-angular:xliffmerge",
    "start-en": "ng serve --proxy-config proxy.conf.json --configuration=en"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!ol|proj4)/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/app/app.constants.spec.ts",
      "<rootDir>/src/app/app.configuration.spec.ts",
      "\\.factory.spec.ts$",
      "\\.utils.spec.ts$"
    ],
    "transform": {
      "(?!<rootDir>/node_modules)/.*\\.js$": "babel-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "^notifications$": "<rootDir>/projects/notifications/src/public_api.ts",
      "^notifications/(.*)$": "<rootDir>/projects/notifications/src/$1"
    }
  },
  "babelOptions": {
    "compact": false,
    "exclude": "/node_modules/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.4",
    "@angular/cdk": "^7.2.4",
    "@angular/common": "^7.2.4",
    "@angular/compiler": "^7.2.4",
    "@angular/core": "^7.2.4",
    "@angular/forms": "^7.2.4",
    "@angular/http": "^7.2.4",
    "@angular/platform-browser": "^7.2.4",
    "@angular/platform-browser-dynamic": "^7.2.4",
    "@angular/router": "^7.2.4",
    "@datorama/akita": "^4.17.1",
    "@datorama/akita-ng-forms-manager": "^3.3.0",
    "@datorama/akita-ng-router-store": "^4.0.0",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@ng-stack/forms": "^1.2.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "angular-resizable-element": "^3.3.2",
    "core-js": "^2.6.4",
    "cropperjs": "^1.5.9",
    "date-fns": "^2.0.0-alpha.27",
    "jspdf": "^1.5.3",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.31",
    "ng-pick-datetime": "^7.0.0",
    "ng-recaptcha": "^5.0.0",
    "ngx-take-until-destroy": "^5.4.0",
    "ngx-ui-loader": "^7.2.2",
    "ol": "^5.3.3",
    "proj4": "^2.5.0",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-builders/jest": "^7.3.1",
    "@angular-devkit/build-angular": "^0.8.9",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "^7.0.7",
    "@angular/compiler-cli": "^7.2.4",
    "@angular/language-service": "^7.2.4",
    "@angularclass/hmr": "^2.1.3",
    "@babel/preset-env": "^7.4.2",
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@datorama/akita-ngdevtools": "^3.0.2",
    "@fortawesome/fontawesome-free": "^5.7.1",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.2",
    "@ngx-i18nsupport/tooling": "^1.1.2",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "~8.9.4",
    "@types/ol": "^5.3.3",
    "@types/openlayers": "^4.6.17",
    "@types/proj4": "^2.5.0",
    "akita-schematics": "^2.0.1",
    "babel-jest": "^24.5.0",
    "bootstrap": ">=4.3.1",
    "codelyzer": "~4.2.1",
    "cypress": "^3.2.0",
    "cypress-file-upload": "^4.0.7",
    "cypress-promise": "^1.1.0",
    "downloads-folder": "^1.0.2",
    "factory.ts": "^0.4.5",
    "jest": "^24.5.0",
    "jest-canvas-mock": "^2.2.0",
    "jest-marbles": "^2.3.1",
    "ng-node-environment": "^1.1.1",
    "ng-packagr": "^4.2.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~3.1.6",
    "wait-for-expect": "^3.0.2"
  }
}
