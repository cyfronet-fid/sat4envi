{
  "name": "s4e-angular",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc",
    "gen-vars": "node node_modules/ng-node-environment/index.js",
    "make-requirements": "npm run gen-vars",
    "prestart": "npm run make-requirements",
    "start": "ng serve --configuration hmr",
    "start-e2e": "ng serve --port 4202 --proxy-config proxy-e2e.conf.json --configuration hmr",
    "prestart-no-persistance": "npm run make-requirements",
    "start-no-persistance": "ng serve --configuration hmr-no-persist --proxy-config proxy.conf.json",
    "prestart-private": "npm run make-requirements",
    "start-private": "ng serve --proxy-config proxy.conf.json --configuration hmr.private",
    "prestart-production": "npm run make-requirements",
    "start-production": "ng serve --proxy-config proxy.conf.json --configuration production",
    "prestart-production-private": "npm run make-requirements",
    "start-production-private": "ng serve --proxy-config proxy.conf.json --configuration production.private",
    "prebuild": "npm run make-requirements",
    "build": "ng build --prod",
    "prebuild-private": "npm run make-requirements",
    "build-private": "ng build --configuration production.private",
    "test": "ng test",
    "lint": "prettier -c src cypress",
    "lint:fix": "prettier -w src cypress",
    "e2e": "CYPRESS_NO_COMMAND_LOG=1 cypress run --browser chrome --headless --quiet",
    "e2e-browser": "CYPRESS_NO_COMMAND_LOG=1 cypress run --browser chrome --quiet",
    "extract-i18n": "ng xi18n s4e-angular --i18n-format xlf --output-path i18n --i18n-locale pl && ng run s4e-angular:xliffmerge",
    "start-en": "ng serve --proxy-config proxy.conf.json --configuration=en"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!proj4)/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/cypress/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/app/app.constants.spec.ts",
      "<rootDir>/src/app/app.configuration.spec.ts",
      "\\.factory.spec.ts$",
      "\\.utils.spec.ts$"
    ],
    "transform": {
      "(?!<rootDir>/node_modules)/.*\\.ts$": "ts-jest",
      "(?!<rootDir>/node_modules)/.*\\.js$": "babel-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ]
  },
  "babelOptions": {
    "compact": false,
    "exclude": "/node_modules/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.1.2",
    "@angular/cdk": "^7.2.4",
    "@angular/common": "^11.1.2",
    "@angular/compiler": "^11.1.2",
    "@angular/core": "^11.1.2",
    "@angular/forms": "^11.1.2",
    "@angular/localize": "~11.1.2",
    "@angular/platform-browser": "^11.1.2",
    "@angular/platform-browser-dynamic": "^11.1.2",
    "@angular/router": "^11.1.2",
    "@datorama/akita": "^4.17.1",
    "@datorama/akita-ng-forms-manager": "^3.3.0",
    "@datorama/akita-ng-router-store": "^4.0.0",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@ng-stack/forms": "^2.3.0",
    "@ngneat/until-destroy": "^8.0.3",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "angular-resizable-element": "^3.3.2",
    "canvg": "2.0.0",
    "core-js": "^2.6.4",
    "cropperjs": "^1.5.9",
    "date-fns": "^2.0.0-alpha.27",
    "jspdf": "^1.5.3",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.31",
    "ng-recaptcha": "^7.0.1",
    "ngx-bootstrap": "^6.2.0",
    "ngx-ui-loader": "^7.2.2",
    "ol": "^5.3.3",
    "proj4": "^2.7.0",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.0.0",
    "@angular-devkit/build-angular": "~0.1101.4",
    "@angular/cli": "^11.1.4",
    "@angular/compiler-cli": "^11.1.2",
    "@angular/language-service": "^11.1.2",
    "@angularclass/hmr": "^2.1.3",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.12.13",
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@datorama/akita-ngdevtools": "^3.0.2",
    "@fortawesome/fontawesome-free": "^5.7.1",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.2",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^12.11.1",
    "@types/ol": "^5.3.3",
    "@types/openlayers": "^4.6.17",
    "@types/proj4": "^2.5.0",
    "akita-schematics": "^2.0.1",
    "babel-jest": "^26.6.3",
    "bootstrap": ">=4.3.1",
    "codelyzer": "^6.0.1",
    "cypress": "^6.4.0",
    "cypress-failed-log": "^2.9.1",
    "cypress-file-upload": "^4.1.1",
    "cypress-intellij-reporter": "0.0.5",
    "cypress-log-to-output": "^1.1.2",
    "cypress-promise": "^1.1.0",
    "downloads-folder": "^1.0.2",
    "factory.ts": "^0.4.5",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "jest-marbles": "^2.3.1",
    "ng-node-environment": "^1.1.1",
    "prettier": "^2.2.1",
    "ts-node": "~5.0.1",
    "tsickle": "^0.39.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.6",
    "wait-for-expect": "^3.0.2"
  }
}
